<!DOCTYPE html>
<% i18n_cache(:suffix=>'application-1') do %>
<html>
<head>
<% end %>
<% if @page_title.nil? then %>
  <title>仲観派</title>
<% else %>
  <title><%=@page_title.to_s%></title>
<% end %>
<% i18n_cache(:suffix=>'application-2') do %>
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
  <meta http-equiv="Content-Style-Type" content="text/css">
  <meta http-equiv="Content-Script-Type" content="text/javascript">
  <meta http-equiv="Pragma" content="no-cache">
  <meta http-equiv="Cache-Control" content="no-cache">
  <meta http-equiv="Expires" content="0">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta charset="utf-8">
  <meta name="description" content="">
  <meta name="keywords" content="">
  <meta name="robots" content="noindex,nofollow">
  <%= favicon_link_tag 'favicon.ico', :type=>'image/png' %>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
<% end %>
  <%= csrf_meta_tags %>
<% i18n_cache(:suffix=>'application-3') do %>
</head>
<body leftmargin="0" topmargin="0">
	<table width="100%" align="center" border="0" cellpadding="0" cellspacing="0">
	<tbody>
	<tr>
		<td align="center">
<% end %>

<%= yield %>

<% i18n_cache(:suffix=>'application-4') do %>
		</td>
	</tr>
	</tbody>
	</table>
</body>
</html>
<% end %>
